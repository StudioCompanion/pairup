{
  "name": "@pairup/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev src/server/index.ts -p ./tsconfig.json",
    "start": "node dist/server/index.js",
    "build": "npx tsc -p tsconfig.prod.json",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env GENERATE=true cross-env DOTENV_CONFIG_PATH=.env.example ts-node-dev --require dotenv/config --transpile-only -P nexus.tsconfig.json ./src/graphql/schema.ts",
    "predev": "yarn run generate",
    "prebuild": "yarn run generate",
    "prisma:migrate": "yarn prisma migrate dev --preview-feature",
    "prisma:studio": "prisma studio",
    "test:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@pairup/shared": "1.0.0",
    "@prisma/client": "^3.8.1",
    "@sanity/client": "^3.2.0",
    "@sanity/webhook": "^1.0.2",
    "@sentry/node": "^6.17.1",
    "airtable": "^0.11.1",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "^3.6.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "express": "^4.17.2",
    "firebase-admin": "^10.0.2",
    "graphql": "15.8.0",
    "graphql-iso-date": "^3.6.1",
    "groq": "^2.15.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.2.0",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.35.0",
    "postmark": "^2.8.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "cross-env": "^7.0.3",
    "prisma": "^3.8.1",
    "sanity-codegen": "^0.9.8",
    "ts-node-dev": "^1.1.8"
  },
  "prisma": {
    "schema": "src/db/schema.prisma"
  }
}
